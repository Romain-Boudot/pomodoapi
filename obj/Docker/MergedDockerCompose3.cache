services:
  pomodoapi:
    build:
      context: C:\Users\Romain\Documents\Projects\pomodoapi
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: pomodoapi
      target: base
    container_name: pomodoapi
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: pomodoapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "bin/Debug/netcoreapp3.1/pomodoapi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5555
      target: 80
    tty: true
    volumes:
    - /c/Users/Romain/Documents/Projects/pomodoapi:/app:rw
    - /c/Users/Romain/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/Romain/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/Romain/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Users/Romain/.nuget/packages:/root/.nuget/packages:ro
    - /c/Users/Romain/Documents/Projects/pomodoapi:/src:rw
version: '3.4'